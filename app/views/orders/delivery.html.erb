<div id="delivFormDiv">
	<h1>Please enter your delivery information:</h1>
	<%= form_tag "/orders/new", method: :get do %>
		<p><%= label_tag "Name:" %>
		<%= text_field_tag :xShipLastName %></p>
		<p><%= label_tag "Street:" %>
		<%= text_field_tag :xShipStreet %></p>
		<p><%= label_tag "City:" %>
		<%= text_field_tag :xShipCity %></p>
		<p><%= label_tag "Zip:" %>
		<%= text_field_tag :xShipZip %></p>
		<%= submit_tag "Continue" %>
	<% end %>
</div>
